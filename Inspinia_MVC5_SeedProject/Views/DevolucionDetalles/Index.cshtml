@model IEnumerable<ERP_ZORZAL.Models.tbDevolucionDetalle>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Detalle Devolución</h2>
        <ol class="breadcrumb">
            @*<li class="active">
                <strong>tbDevolucionDetalle</strong>
            </li>*@
        </ol>
    </div>
</div>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Lista de Detalle Devolución</h5>
					<div class="ibox-tools">
						@Html.ActionLink("Nuevo", "Create", null, new { @class = "btn btn-primary btn-xs"})
					</div>
                </div>
                <div class="ibox-content">

<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.dev_Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.prod_Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ddv_CantidadProducto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ddv_Descripcion)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.dev_Codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.prod_Codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ddv_CantidadProducto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ddv_Descripcion)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.ddv_Codigo }, new { @class = "btn btn-white btn-sm"})
            @Html.ActionLink("Eliminar", "Delete", new { id=item.ddv_Codigo }, new { @class = "btn btn-white btn-sm"})
        </td>
    </tr>
}

</table>

                </div>
            </div>
        </div>
    </div>
 </div>



