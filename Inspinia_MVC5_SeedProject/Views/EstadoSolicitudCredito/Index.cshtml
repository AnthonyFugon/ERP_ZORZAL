@model IEnumerable<ERP_ZORZAL.Models.tbEstadoSolicitudCredito>

@{ 
    ViewBag.Title = "Estado Solicitud Crèdito";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Estado Solicitud Crèdito</h2>
        @*<ol class="breadcrumb">
            <li class="active">
                <strong>tbEstadoSolicitudCredito</strong>
            </li>*@
        @*</ol>*@
    </div>
</div>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Listado Solicitud Credito</h5>
					<div class="ibox-tools">
						@Html.ActionLink("Nuevo", "Create", null, new { @class = "btn btn-primary btn-xs"})
					</div>
                </div>
                <div class="ibox-content">

<table class="table table-striped table-bordered table-hover" id="DataTable">
    <thead>

        <tr>
            @*<th>*@
                Estado 
                @*@Html.DisplayNameFor(model => model.escre_Id)*@
            @*</th>
            <th>*@
                Descripción
                @*@Html.DisplayNameFor(model => model.escre_Descripcion)*@
            @*</th>
            <th>*@
                @*Usuario Crea*@ 
                @*@Html.DisplayNameFor(model => model.escre_UsuarioCrea)*@
            @*</th>
            <th>*@
                @*Usuarios Modifico*@ 
                @*@Html.DisplayNameFor(model => model.escre_UsuarioModifico)*@
            @*</th>
            <th>*@
                @*Fecha  Agrego*@
                @*@Html.DisplayNameFor(model => model.escre_FechaAgrego)*@
            @*</th>*@
            @*<th>*@
                @*Fecha Modifico*@
                @*@Html.DisplayNameFor(model => model.escre_FechaModifico)*@
            @*</th>*@
            <th></th>
        </tr>
    </thead>
    <tbody>
    
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.escre_Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.escre_UsuarioCrea)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.escre_UsuarioModifico)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.escre_FechaAgrego)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.escre_FechaModifico)
        </td>
        <td>
			@Html.ActionLink("Details", "Details", new { id=item.escre_Id }, new { @class = "btn btn-primary btn-sm"})
            @Html.ActionLink("Edit", "Edit", new { id=item.escre_Id }, new { @class = "btn btn-white btn-sm"})
            @Html.ActionLink("Delete", "Delete", new { id=item.escre_Id }, new { @class = "btn btn-white btn-sm"})
        </td>
    </tr>
}
        </tbody>
</table>

                </div>
            </div>
        </div>
    </div>
 </div>






<script src="~/Scripts/jquery-3.3.1.js"></script>
