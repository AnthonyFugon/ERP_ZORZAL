
@model Inspinia_MVC5_SeedProject.Models.tbObjeto

@{
    ViewBag.Title = "Detalles";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Detalles</h2>
        <ol class="breadcrumb">
            <li>
                @Html.ActionLink("Listado", "Index")
            </li>
            <li class="active">
                <strong>Detalles</strong>
            </li>
        </ol>
    </div>
</div>




<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalles Objetos</h5>
                </div>
                <div class="ibox-content">


                    <div class="form-horizontal">

                        <div class="form-group">
                            <label class="control-label col-md-2">Objeto Pantalla</label>

                            <div class="col-md-10">
                                <input type="text" class="text-box single-line" value="1" readonly="readonly" />
                            </div>
                        </div>

                    @Html.ActionLink("Editar", "EditarPrueba", new { @class = "btn btn-primary" })
                    @Html.ActionLink("Regresar a lista", "Index", null, new { @class = "btn btn-white" })

                </div>
            </div>
        </div>
    </div>
</div>












