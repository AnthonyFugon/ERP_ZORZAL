    </div>
 </div>

<!-- INSERT MODAL -->
<div class="modal fade" id="modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLabel"><label id="LblSearchByDescription">Editar</label></h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-12">
                        <div class="form-group row">
                            <label style="margin-left:15px" class="col-sm-4 col-form-label" id="LblDescription">ID del Acceso del rol</label>
                            <div class="col-sm-7  input-group">
                                <input type="text" class="form-control" id="txtIDTipoTaller" value="1" readonly />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label style="margin-left:15px" class="col-sm-4 col-form-label" id="LblDescription1">Id del rol</label>
                            <div class="col-sm-7  input-group">
                                <input type="text" class="form-control" id="txtNombreTipoTaller" value="1"/>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label style="margin-left:15px" class="col-sm-4 col-form-label" id="LblDescription1">Id del objeto</label>
                            <div class="col-sm-7  input-group">
                                <input type="text" class="form-control" id="txtNombreTipoTaller" value="1"/>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label style="margin-left:15px" class="col-sm-4 col-form-label" id="LblDescription1">Descripcion del acceso por rol</label>
                            <div class="col-sm-7  input-group">
                                <input type="text" class="form-control" id="txtNombreTipoTaller" value="prueba"/>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label style="margin-left:15px" class="col-sm-4 col-form-label" id="LblDescription1">Creado por:</label>
                            <div class="col-sm-7  input-group">
                                <input type="text" class="form-control" value="Antonio" readonly />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label style="margin-left:15px" class="col-sm-4 col-form-label" id="LblDescription1">Creado en:</label>
                            <div class="col-sm-7  input-group">
                                <input type="text" class="form-control" value="29/11/2018" readonly />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label style="margin-left:15px" class="col-sm-4 col-form-label" id="LblDescription1">Modificado por:</label>
                            <div class="col-sm-7  input-group">
                                <input type="text" class="form-control" value="Antonio" readonly />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label style="margin-left:15px" class="col-sm-4 col-form-label" id="LblDescription1">Modificado en:</label>
                            <div class="col-sm-7  input-group">
                                <input type="text" class="form-control" value="29/11/2018" readonly />
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <input type="button" class="btn btn-primary" data-dismiss="modal" style="width:110px;" id="btnCerrar" value="Cerrar" />
                <input type="button" class="btn btn-primary" id="btnAceptar" style="width:110px;" value="Aceptar" onclick="Aceptar();" />
            </div>
        </div>
    </div>
</div>

