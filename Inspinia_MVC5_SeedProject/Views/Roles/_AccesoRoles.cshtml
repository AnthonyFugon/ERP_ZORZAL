<table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.acrol_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.rol_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.obj_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.acrol_Descripcion)
        </th>
