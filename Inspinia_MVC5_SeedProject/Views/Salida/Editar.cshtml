@model ERP_ZORZAL.Models.tbSalida


<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Editar</h2>
        <ol class="breadcrumb">
            <li>
                @Html.ActionLink("Listado", "Index_Salida")
            </li>
            <li class="active">
                <strong>Editar</strong>
            </li>
        </ol>
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            @Html.ActionLink("Regresar", "Index_Salida", null, new { @class = "btn btn-primary" })
        </div>
    </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">

                <div class="ibox-content">

                    <div class="form-horizontal">

                        <div class="form-group">
                            <label class="control-label col-md-2">Codigo Salida</label>
                            <div class="col-md-10">
                                <input type="text" @*class="text-box single-line"*@ readonly="readonly" value="1" width="200" height="200"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-2">Bodega</label>
                            <div class="col-md-10">
                                <input type="text" @*class="text-box single-line"*@ value="Tegucigalpa" width="200" height="200"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-2">Codigo Factura</label>
                            <div class="col-md-10">
                                <input type="text" @*class="text-box single-line"*@ value="0000" width="200" height="200"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-2">Fecha Salida</label>
                            <div class="col-md-10">
                                <input type="text" @*class="text-box single-line"*@ value="12/12/12" width="200" height="200"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-2">Estado Movimiento</label>
                            <div class="col-md-10">
                                <input type="text" @*class="text-box single-line"*@ value="Activo " width="200" height="200"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-2">Destino Salida</label>
                            <div class="col-md-10">
                                <input type="text" @*class="text-box single-line"*@ value="La Ceiba" width="200" height="200"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-2">Codigo Caja</label>
                            <div class="col-md-10">
                                <input type="text" @*class="text-box single-line"*@ value="000" width="200" height="200"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-2">Creado Por</label>
                            <div class="col-md-10">
                                <input type="text" @*class="text-box single-line"*@ value="Mafer" width="200" height="200" readonly="readonly"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-2">Modificado Por</label>
                            <div class="col-md-10">
                                <input type="text" @*class="text-box single-line"*@ value="Mafer" width="200" height="200" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-2">Creado el</label>
                            <div class="col-md-10">
                                <input type="text" @*class="text-box single-line"*@ value="20/11/18" width="200" height="200" readonly="readonly"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="control-label col-md-2">Modificado el</label>
                            <div class="col-md-10">
                                <input type="text" @*class="text-box single-line"*@ value="23/11/18" width="200" height="200" />
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-offset-2 col-md-10">
                                <input type="submit" value="Guardar" class="btn btn-primary" />
                                @Html.ActionLink("Cancelar", "Index_Salida", null, new { @class = "btn btn-white" })
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
