<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Listado Salida Detalle</h5>
					<div class="ibox-tools">
						@Html.ActionLink("Crear Nuevo", "Create", null, new { @class = "btn btn-primary btn-xs"})
					</div>
                </div>
                <div class="ibox-content">

<table id="ListaCliente" class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.bod_Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.bod_ResponsableBodega)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.sald_UsuarioCrea)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.sald_FechaCrea)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.sald_UsuarioModifica)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.sald_FechaModifica)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.tbProducto.prod_Descripcion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.bod_Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.bod_ResponsableBodega)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sald_UsuarioCrea)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sald_FechaCrea)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sald_UsuarioModifica)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sald_FechaModifica)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.tbProducto.prod_Descripcion)
                </td>
                <td>
                    @Html.ActionLink("Details", "Details", new { id = item.sald_Id }, new { @class = "btn btn-primary btn-sm" })
                    @Html.ActionLink("Edit", "Edit", new { id = item.sald_Id }, new { @class = "btn btn-white btn-sm" })
                    @Html.ActionLink("Delete", "Delete", new { id = item.sald_Id }, new { @class = "btn btn-white btn-sm" })
                </td>
            </tr>
        }
    </tbody>
</table>

