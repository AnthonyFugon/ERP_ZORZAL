        <th>
            @*@Html.DisplayNameFor(model => model.tbCaja.cja_Descripcion)*@
            Descripcion Caja
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.aqcja_Fecha)
