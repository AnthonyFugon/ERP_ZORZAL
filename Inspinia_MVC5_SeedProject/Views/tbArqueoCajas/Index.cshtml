@model IEnumerable<ERP_ZORZAL.Models.tbArqueoCaja>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Index</h2>
        <ol class="breadcrumb">
            <li class="active">
                <strong>Arqueo Caja</strong>
            </li>
        </ol>
    </div>
</div>


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>List of tbArqueoCaja</h5>
					<div class="ibox-tools">
						@Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-primary btn-xs"})
					</div>
                </div>
                <div class="ibox-content">

<table class="table table-striped">
    <tr>
        <th>
            Fecha de Arqueo
            @*@Html.DisplayNameFor(model => model.aqcja_Fecha)*@
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.aqcja_HoraInicio)*@
            Hora Inicio
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.aqcja_HoraFinal)*@
            Hora Final
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.aqcja_SaldoInicial)*@
