@model Inspinia_MVC5_SeedProject.Models.tbEntrada



<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Detalles</h5>
                </div>
                <div class="ibox-content">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <div class="col-md-10">
                                <label class="control-label col-md-2">Numero Factura</label>
                                <div class="col-md-6">
                                    <input class="text-box single-line" value="15610200" readonly />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-10">
                                <label class="control-label col-md-2">Productos</label>
                                <div class="col-md-6">
                                    <input class="text-box single-line" value="Camisas" readonly />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-10">
                                <label class="control-label col-md-2">Unidad De medida</label>
                                <div class="col-md-6">
                                    <input class="text-box single-line" value="100" readonly />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-10">
                                <label class="control-label col-md-2">Precio Unitario</label>
                                <div class="col-md-6">
                                    <input class="text-box single-line" value="500.00" readonly />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-10">
                                <label class="control-label col-md-2">Fecha Entrada</label>
                                <div class="col-md-6">
                                    <input class="text-box single-line" value="24/11/2018" readonly />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-10">
                                <label class="control-label col-md-2">Bodega Descpacho</label>
                                <div class="col-md-6">
                                    <input class="text-box single-line" value="Bodega S.P.S" readonly />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-10">
                                <label class="control-label col-md-2">Usuario</label>
                                <div class="col-md-6">
                                    <input class="text-box single-line" value="Juan Perez" readonly />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-offset-5 col-md-10">
                                @Html.ActionLink("Regresar", "Index", null, new { @class = "btn btn-white" })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



