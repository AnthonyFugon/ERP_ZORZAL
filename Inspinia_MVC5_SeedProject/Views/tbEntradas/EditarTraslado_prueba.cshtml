@model Inspinia_MVC5_SeedProject.Models.tbEntrada

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Entrada Por Traslado</h2>
        <ol class="breadcrumb">
            <li>
                @Html.ActionLink("Volver", "Index")
            </li>
        </ol>
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            @Html.ActionLink("Regresar", "Index", null, new { @class = "btn btn-primary" })
        </div>
    </div>
</div>




<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Editar</h5>
                </div>
                <div class="ibox-content">
                    <div class="form-horizontal">
                        <div class="form-group">
                            <div class="col-md-10">
                                <label class="control-label col-md-2">Productos</label>
                                <div class="col-md-6">
                                    <input class="text-box single-line" value="Camisas" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-10">
                                <label class="control-label col-md-2">Unidad De medida</label>
                                <div class="col-md-6">
                                    <select class="form-control" id="" name="">
                                        <option>Seleccione Unidad De Medida</option>
                                        <option>Docena</option>
                                        <option>Media docena</option>
                                        <option>Kilogramos</option>
                                        <option>Libras</option>
                                        <option>Gramos</option>
                                        <option>Unidades</option>
                                        <option>Litros</option>
                                        <option>Galones</option>
                                        <option>Barriles</option>
                                        <option>Latas</option>
                                        <option>Cajas</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-10">
                                <label class="control-label col-md-2">Precio Unitario</label>
                                <div class="col-md-6">
                                    <input class="text-box single-line" value="500.00" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-10">
                                <label class="control-label col-md-2">Fecha Entrada</label>
                                <div class="col-md-6">
                                    <input class="text-box single-line" value="24/11/2018" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-10">
                                <label class="control-label col-md-2">Bodega Descpacho</label>
                                <div class="col-md-6">
                                    <select class="form-control" id="" name="">
                                        <option>Seleccione Bodega</option>
                                        <option>San Pedro Sula</option>
                                        <option>La Ceiba</option>
                                        <option>Tegucigalpa</option>
                                        <option>Choloma</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-10">
                                <label class="control-label col-md-2">Usuario</label>
                                <div class="col-md-6">
                                    <input class="text-box single-line" value="Juan Perez" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-offset-2 col-md-10">
                                <input type="submit" value="Guardar" class="btn btn-primary" />
                                @Html.ActionLink("Cancelar", "Index", null, new { @class = "btn btn-white" })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>