@model Inspinia_MVC5_SeedProject.Models.tbSubcategoria
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Edit</h2>
        <ol class="breadcrumb">
            <li>
                @Html.ActionLink("Lista", "Index")
            </li>
            <li class="active">
                <strong>Editar</strong>
            </li>
        </ol>
    </div>
    <div class="col-sm-8">
        <div class="title-action">
            @Html.ActionLink("Regresar", "Index", null, new { @class = "btn btn-primary" })
        </div>
    </div>
</div>
 



    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="row">
            <div class="col-lg-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5>Edit tbSubcategoria</h5>
                    </div>
                    <div class="ibox-content">



                        <div class="form-horizontal">


                            <div class="form-group">

                                <div class="col-md-10">
                                    <label class="control-label col-md-2"> Id Sub Categoria</label>
                                    <div class="col-md-10">
                                        <input class="text-box single-line" value="1" />
                                    </div>
                                </div>
                            </div>

                            <div class="form-group">
                                <div class="col-md-10">
                                    <label class="control-label col-md-2">Descricpion</label>
                                    <div class="col-md-10">
                                        <input class="text-box single-line" value="Pantalon Jean" />
                                    </div>
                                </div>
                               

                        </div>
                            <div class="form-group">
                                <div class="col-md-offset-2 col-md-10">
                                    <input type="submit" value="Save" class="btn btn-primary" />
                                    @Html.ActionLink("Cancel", "Index", null, new { @class = "btn btn-white" })
                                </div>
                            </div>
                        </div>
                    </div>
            </div>
        </div>
    </div>
</div>

